source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'
use_frameworks!

target 'LocatrSwift' do
  pod 'Alamofire', '~> 3.0' # For making http requests
  pod 'ObjectMapper', '~> 1.1' # Easy way to map Json data to Plain Old Swift Objects
  pod 'Kingfisher', '~> 2.0' # Easy way to fetch images (for user profile pictures)
end
